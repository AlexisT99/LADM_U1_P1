<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background=" #20cf9a"
    android:orientation="vertical"
    tools:context=".MainActivity5">

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingBottom="40dp">

        <TextView
            android:id="@+id/textView9"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="20dp"
            android:layout_marginTop="30dp"
            android:text="Card"
            android:textColor="#FFFFFF"
            android:textSize="46sp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/textView10"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="20dp"
            android:layout_marginTop="90dp"
            android:text="Simply and easy to use app"
            android:textColor="#FFFFFF"
            android:textSize="18sp" />

        <ImageView
            android:id="@+id/imageView"
            android:layout_width="85dp"
            android:layout_height="83dp"
            android:layout_gravity="end"
            android:paddingTop="10dp"
            app:srcCompat="@drawable/imagensinfondo" />

    </FrameLayout>

    <TableLayout
        android:layout_width="match_parent"
        android:layout_height="530dp"
        android:paddingTop="20dp"
        android:paddingBottom="20dp">

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:paddingBottom="10dp">

            <FrameLayout
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_marginStart="20dp"
                android:layout_marginEnd="13dp"
                android:background="@drawable/tarjeta"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/textView11"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="bottom|center"
                    android:paddingBottom="15dp"
                    android:text="Text"
                    android:textSize="24sp"
                    android:textStyle="bold" />

                <ImageView
                    android:id="@+id/imageView8"
                    android:layout_width="90dp"
                    android:layout_height="86dp"
                    android:layout_gravity="center"
                    app:srcCompat="@drawable/study" />

                <TextView
                    android:id="@+id/textView18"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="bottom|center"
                    android:text="Desripción de la card" />
            </FrameLayout>

            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginEnd="10sp"
                android:background="@drawable/tarjeta"
                android:orientation="horizontal"
                android:paddingStart="20dp"
                android:paddingEnd="0dp" >
                <TextView
                    android:id="@+id/textView12"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="bottom|center"
                    android:paddingBottom="15dp"
                    android:text="Address"
                    android:textSize="24sp"
                    android:textStyle="bold"
                    android:paddingEnd="10dp"/>

                <ImageView
                    android:id="@+id/imageView9"
                    android:layout_width="82dp"
                    android:layout_height="81dp"
                    android:layout_gravity="center"
                    android:paddingBottom="10dp"
                    app:srcCompat="@drawable/address"
                    android:paddingEnd="10dp"/>

                <TextView
                    android:id="@+id/textView19"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="bottom|center"
                    android:paddingEnd="15dp"
                    android:text="Desripción de la card" />
            </FrameLayout>
        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:paddingBottom="10dp">

            <FrameLayout
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_marginStart="20dp"
                android:layout_marginEnd="13dp"
                android:background="@drawable/tarjeta"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/textView13"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="bottom|center"
                    android:paddingBottom="15dp"
                    android:text="Character"
                    android:textSize="24sp"
                    android:textStyle="bold" />

                <ImageView
                    android:id="@+id/imageView10"
                    android:layout_width="82dp"
                    android:layout_height="81dp"
                    android:layout_gravity="center"
                    android:paddingBottom="10dp"
                    app:srcCompat="@drawable/character" />
                <TextView
                    android:id="@+id/textView20"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="bottom|center"
                    android:text="Desripción de la card" />

            </FrameLayout>

            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginEnd="10sp"
                android:background="@drawable/tarjeta"
                android:orientation="horizontal"
                android:paddingStart="20dp"
                android:paddingEnd="0dp" >

                <TextView
                    android:id="@+id/textView14"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="bottom|center"
                    android:paddingBottom="15dp"
                    android:text="Bank Card"
                    android:textSize="24sp"
                    android:textStyle="bold"
                    android:paddingEnd="10dp"/>

                <ImageView
                    android:id="@+id/imageView11"
                    android:layout_width="82dp"
                    android:layout_height="81dp"
                    android:layout_gravity="center"
                    android:paddingBottom="10dp"
                    app:srcCompat="@drawable/credit_card"
                    android:paddingEnd="10dp"/>

                <TextView
                    android:id="@+id/textView21"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="bottom|center"
                    android:paddingEnd="15dp"
                    android:text="Desripción de la card" />
            </FrameLayout>
        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <FrameLayout
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_marginStart="20dp"
                android:layout_marginEnd="13dp"
                android:background="@drawable/tarjeta"
                android:orientation="horizontal">
                <TextView
                    android:id="@+id/textView15"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="bottom|center"
                    android:paddingBottom="15dp"
                    android:text="Password"
                    android:textSize="24sp"
                    android:textStyle="bold" />

                <ImageView
                    android:id="@+id/imageView12"
                    android:layout_width="82dp"
                    android:layout_height="81dp"
                    android:layout_gravity="center"
                    android:paddingBottom="10dp"
                    app:srcCompat="@drawable/password" />
                <TextView
                    android:id="@+id/textView22"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="bottom|center"
                    android:text="Desripción de la card" />
            </FrameLayout>

            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginEnd="10sp"
                android:background="@drawable/tarjeta"
                android:orientation="horizontal"
                android:paddingStart="20dp"
                android:paddingEnd="0dp" >
                <TextView
                    android:id="@+id/textView16"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="bottom|center"
                    android:paddingBottom="15dp"
                    android:text="Logistics"
                    android:textSize="24sp"
                    android:textStyle="bold"
                    android:paddingEnd="10dp"/>

                <ImageView
                    android:id="@+id/imageView13"
                    android:layout_width="82dp"
                    android:layout_height="81dp"
                    android:layout_gravity="center"
                    android:paddingBottom="10dp"
                    app:srcCompat="@drawable/logistics"
                    android:paddingEnd="10dp"/>

                <TextView
                    android:id="@+id/textView23"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="bottom|center"
                    android:paddingEnd="15dp"
                    android:text="Desripción de la card" />
            </FrameLayout>
        </TableRow>
    </TableLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginStart="15dp"
        android:layout_marginEnd="12dp"
        android:layout_marginBottom="20dp"
        android:background="@drawable/tarjeta2">

        <ImageView
            android:id="@+id/imageView21"
            android:layout_width="214dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:paddingStart="20dp"
            app:srcCompat="@drawable/gear" />

        <TextView
            android:id="@+id/textView17"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:gravity="center|center_vertical"
            android:text="Settings"
            android:textSize="36sp" />

    </LinearLayout>

</LinearLayout>